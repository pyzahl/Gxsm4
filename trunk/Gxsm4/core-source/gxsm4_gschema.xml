 <?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/org/gnome/gxsm4/" id="org.gnome.gxsm4">
    <key name="test-font" type="s">
      <default>'Monospace 12'</default>
      <summary>Font</summary>
      <description>The font to be used for content.</description>
    </key>
    <key name="test-transition" type="s">
      <choices>
        <choice value='none'/>
        <choice value='crossfade'/>
        <choice value='slide-left-right'/>
      </choices>
      <default>'none'</default>
      <summary>Transition</summary>
      <description>The transition to use when switching tabs.</description>
    </key>
  </schema>
  <schema path="GUI-global/System" id="GUI-global.System">
    <key name="First-Start-Key" type="b">
      <default>'TRUE'</default>
      <description>GXSM4 first startup indicator.</description>
    </key>
  </schema>
</schemalist>
